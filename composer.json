{
	"name":"fritzmg/contao-facebook-js-sdk",
	"description":"Automatically integrates the Facebook JavaScript SDK in the Contao frontend or backend",
	"keywords":["contao", "facebook", "sdk", "javascript"],
	"type":"contao-module",
	"homepage":"http://www.inspiredminds.at",
	"license":"GPL-2.0",
	"authors":[
		{
			"name":"Fritz Michael Gschwantner",
			"homepage":"http://www.inspiredminds.at",
			"email":"fmg@inspiredminds.at",
			"role":"Developer"
		}
	],
	"support":{
		"email":"fmg@inspiredminds.at",
		"issues":"https://github.com/fritzmg/contao-facebook-js-sdk/issues",
		"source":"https://github.com/fritzmg/contao-facebook-js-sdk",
		"forum":"https://community.contao.org/de"
	},
	"require":{
		"php":">=5.2",
		"contao/core":"~3.0",
		"contao-community-alliance/composer-installer":"*"
	},
    "replace": {
        "contao-legacy/facebook_js_sdk": "self.version"
    },
	"extra":{
		"contao":{
			"sources":{
				"system/modules/facebook_js_sdk":"system/modules/facebook_js_sdk"
			}
		}
	}
}
